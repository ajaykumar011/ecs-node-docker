{
    "executionRoleArn": "arn:aws:iam::143787628822:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "ecs-node-docker",
        "image": "143787628822.dkr.ecr.ap-south-1.amazonaws.com/ecs-node-docker:latest",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 0,
            "protocol": "tcp",
            "containerPort": 3000
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/ecs-node-docker-td",
            "awslogs-region": "ap-south-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": [
      "EC2"
    ],
    "cpu": "256",
    "memory": "256",
    "family": "ecs-node-docker-td"
  }